@article{cribari-neto_inflated_2019,
	title = {Inflated {Kumaraswamy} distributions},
	volume = {91},
	issn = {0001-3765, 1678-2690},
	url = {http://www.scielo.br/j/aabc/a/p63NgxGNxH9Mcd6HYYhdqKM/?lang=en},
